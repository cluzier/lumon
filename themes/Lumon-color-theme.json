"tokenColors": [
	{
		"scope": [
			"comment",
			"punctuation.definition.comment"
		],
		"settings": {
			"foreground": "#587f88",
			"fontStyle": "italic"
		}
	},
	{
		"scope": [
			"string",
			"constant.other.symbol",
			"constant.other.key"
		],
		"settings": {
			"foreground": "#12f2b3"
		}
	},
	{
		"scope": [
			"variable",
			"variable.parameter",
			"variable.language"
		],
		"settings": {
			"foreground": "#79e6ff"
		}
	},
	{
		"scope": [
			"keyword",
			"storage.type",
			"storage.modifier"
		],
		"settings": {
			"foreground": "#ffaaf2",
			"fontStyle": "bold"
		}
	},
	{
		"scope": [
			"constant.numeric",
			"constant.language",
			"constant.character",
			"constant.other"
		],
		"settings": {
			"foreground": "#eedc44"
		}
	},
	{
		"scope": [
			"entity.name.function",
			"support.function",
			"keyword.other.special-method"
		],
		"settings": {
			"foreground": "#4e94ce"
		}
	},
	{
		"scope": [
			"entity.name.type",
			"entity.other.inherited-class",
			"support.class",
			"support.type"
		],
		"settings": {
			"foreground": "#bc3fbc"
		}
	},
	{
		"scope": [
			"entity.name.tag",
			"support.constant",
			"support.variable"
		],
		"settings": {
			"foreground": "#cca700"
		}
	},
	{
		"scope": [
			"meta.function-call",
			"meta.method-call",
			"meta.property-name"
		],
		"settings": {
			"foreground": "#29b8db"
		}
	},
	{
		"scope": [
			"invalid",
			"invalid.illegal"
		],
		"settings": {
			"foreground": "#f14c4c",
			"fontStyle": "underline"
		}
	},
	{
		"scope": [
			"markup.heading",
			"markup.bold",
			"markup.italic"
		],
		"settings": {
			"foreground": "#3b8eea",
			"fontStyle": "bold"
		}
	},
	{
		"scope": [
			"markup.inserted",
			"markup.deleted",
			"markup.changed"
		],
		"settings": {
			"foreground": "#0dbc79"
		}
	}
]
}
